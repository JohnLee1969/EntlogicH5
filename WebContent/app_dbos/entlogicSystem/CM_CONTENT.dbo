{"dsn":"jdbc/entlogic","dboName":"CM_CONTENT","create":{"SQL":"select \n  '' as id,\n  '' as lv,\n  '' as OID,\n  '' as CM_CATALOG,\n  9999 as SN,\n  '00' as CONTENT_TYPE,\n  '' as CONTENT_NAME,\n  '' as HEAD,\n  '' as SUBHEAD,\n  getDate() as START_TIME,\n  '2099-12-31' as END_TIME,\n  '' as URL,\n  '' as SRC,\n  '' as NOTE,\n  '00' as STATUS,\n  '' as FULL_TEXT\nfrom\n  FW_PARAMETER \nwhere \n  OID = 'SYS_APP_NAME'\n","SQL_MSSQL":"","SQL_ORACLE":""},"insert":{"SQL":"insert into CM_CONTENT (\n  OID,\n  CM_CATALOG,\n  SN,\n  CONTENT_TYPE,\n  CONTENT_NAME,\n  HEAD,\n  SUBHEAD,\n  START_TIME,\n  END_TIME,\n  URL,\n  SRC,\n  NOTE,\n  STATUS,\n  FULL_TEXT\n) values (\n  '#OID#',\n  '#CM_CATALOG#',\n  #SN#,\n  '#CONTENT_TYPE#',\n  '#CONTENT_NAME#',\n  '#HEAD#',\n  '#SUBHEAD#',\n  '#START_TIME#',\n  '#END_TIME#',\n  '#URL#',\n  '#SRC#',\n  '#NOTE#',\n  '#STATUS#',\n  '#FULL_TEXT#' \n)\n","SQL_MSSQL":"","SQL_ORACLE":""},"delete":{"SQL":"delete CM_CONTENT where OID = '#OID#'","SQL_MSSQL":"","SQL_ORACLE":""},"update":{"SQL":"update CM_CONTENT set \n  OID =   '#OID#',\n  CM_CATALOG =   '#CM_CATALOG#',\n  SN =   #SN#,\n  CONTENT_TYPE =   '#CONTENT_TYPE#',\n  CONTENT_NAME =   '#CONTENT_NAME#',\n  HEAD =   '#HEAD#',\n  SUBHEAD =   '#SUBHEAD#',\n  START_TIME =   '#START_TIME#',\n  END_TIME =   '#END_TIME#',\n  URL =   '#URL#',\n  SRC =   '#SRC#',\n  NOTE =   '#NOTE#',\n  STATUS =   '#STATUS#',\n  FULL_TEXT =   '#FULL_TEXT#'\nwhere OID = '#id#' \n","SQL_MSSQL":"","SQL_ORACLE":""},"query":{"SQL":"select \n  OID as id,\n  '0' as lv, \n  row_number() over(#ORDER_BY_CLAUSE#) as rn,\n  OID,\n  CM_CATALOG,\n  SN,\n  CONTENT_TYPE,\n  CONTENT_NAME,\n  HEAD,\n  SUBHEAD,\n  START_TIME,\n  dbo.fwTimeToChar(START_TIME) as START_TIME_TEXT,\n  END_TIME,\n  dbo.fwTimeToChar(END_TIME) as END_TIME_TEXT,\n  URL,\n  SRC,\n  NOTE,\n  STATUS,\n  dbo.fwCodeToText('CMS', STATUS) as STATUS_TEXT,\n  FULL_TEXT\nfrom\n  CM_CONTENT\n#WHERE_CLAUSE#\n","SQL_MSSQL":"select \n  OID as id,\n  '0' as lv, \n  row_number() over(#ORDER_BY_CLAUSE#) as rn,\n  OID,\n  CM_CATALOG,\n  SN,\n  CONTENT_TYPE,\n  CONTENT_NAME,\n  HEAD,\n  SUBHEAD,\n  START_TIME,\n  dbo.fwTimeToChar(START_TIME) as START_TIME_TEXT,\n  END_TIME,\n  dbo.fwTimeToChar(END_TIME) as END_TIME_TEXT,\n  URL,\n  SRC,\n  NOTE,\n  STATUS,\n  dbo.fwCodeToText('CMS', STATUS) as STATUS_TEXT,\n  FULL_TEXT\nfrom\n  CM_CONTENT\n#WHERE_CLAUSE#\n","SQL_ORACLE":"select \n  OID as id,\n  '0' as lv, \n  ROWNUM as rn,\n  OID,\n  CM_CATALOG,\n  SN,\n  CONTENT_TYPE,\n  CONTENT_NAME,\n  HEAD,\n  SUBHEAD,\n  START_TIME,\n  fwTimeToChar(START_TIME) as START_TIME_TEXT,\n  END_TIME,\n  fwTimeToChar(END_TIME) as END_TIME_TEXT,\n  URL,\n  SRC,\n  NOTE,\n  STATUS,\n  fwCodeToText('CMS', STATUS) as STATUS_TEXT,\n  FULL_TEXT\nfrom\n  CM_CONTENT\n#WHERE_CLAUSE#\n#ORDER_BY_CLAUSE#\n"}}